  },
  "keywords": [
    "WebdriverIO",
    "debugging",
    "javascript",
    "testing",
    "e2e"
  ],
  "author": "Will Brock",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  }
}
