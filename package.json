{
  "name": "wdio-debugger-service",
  "version": "0.0.10",
  "description": "WebdriverIO service to pause and step through tests",
  "main": "./build/index",
    "repository": {
    "type": "git",
    "url": "https://github.com/WillBrock/wdio-debugger-service.git"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ./babel.config.js",
